    "compilerOptions": {
        "incremental": true,
        "target": "ES2021",
        "module": "CommonJS",
        "outDir": "./build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true
    }
}
